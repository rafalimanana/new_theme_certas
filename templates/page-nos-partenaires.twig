{% extends "_base_page.twig" %}
{% block header %}
{% endblock %}
{% block content %}

<div class="wrapper {{sidebar_class}}">
    <!-- Aide à la navigation -->
    <div class="content-wrapper _bg_white" id="_pg_club-energy">
        <div class="container">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb _bredcrumb_custo">
                    <li class="breadcrumb-item"><a class="a-navig" href="{{ site.url }}">Accueil</a></li>
                    {% if post.parent.parent.title %}
                        <li class="breadcrumb-item"><a class="a-navig" href="{{ post.parent.parent.link }}">{{post.parent.parent.title}}</a></li>
                    {% endif %}
                    {% if post.parent.title %}
                        <li class="breadcrumb-item"><a class="a-navig" href="{{post.parent.link}}">{{post.parent.title}}</a></li>
                    {% endif %}
                    <li class="breadcrumb-item active"><strong>{{post.title}}</strong></li>
                </ol>
            </nav>
        </div>
    </div>
    <!-- Aide à la navigation -->
    <div class="container content-wrapper _pd_tp_50 _pd_bt_50">
        {% for i in 1..10 %}
            {% set club = post.get_field("partenaire_"~i) %}
            {% if i is divisible by(2) %}
                {% if club and (club.premier_titre or club.premier_contenu) %}
                    <div class="row">
                        <div class="col-md-6">
                            {% if club.premier_image %}
                                <img class="img_club" src="{{club.premier_image.url}}">
                            {% endif %}
                        </div>
                        <div class="col-md-6 pl_club class_club_energy">
                            {% if club.premier_titre %}
                                <h2 class="text-uppercase  services-h2">
                                    {{club.premier_titre}}
                                </h2>
                            {% endif %}
                            {% if club.premier_contenu %}
                                {{ club.premier_contenu }}
                            {% endif %}
                            <p class="text-center _pd_tp_40">
                                {% set p_url = club.premier_url_savoir_plus %}
                                {% if club.premier_texte_savoir_plus %}
                                    <a href="{% if p_url.url %}{{p_url.url}}{% else %}#{% endif %}" class="" target="{% if p_url.target %}{{p_url.target}}{% else %}_self{% endif %}">
                                            <button class="btn btn-secondary btn-first">
                                                {{ club.premier_texte_savoir_plus }}
                                            </button>
                                    </a>
                                {% endif %}
                            </p>
                        </div>
                    </div>
                {% endif %}
                {% if club and (club.deuxieme_titre or club.deuxieme_contenu) %}
                    <div class="row mt-5">
                        <div class="col-md-6 class_club_energy">
                            {% if club.deuxieme_titre %}
                                <h2 class="text-uppercase  services-h2">
                                    {{club.deuxieme_titre}}
                                </h2>
                            {% endif %}
                            {% if club.deuxieme_contenu %}
                                {{club.deuxieme_contenu}}
                            {% endif %}
                            {% set d_url = club.deuxieme_url_savoir_plus %}
                            {% if club.deuxieme_texte_savoir_plus %}
                                <p class="text-center _pd_tp_40">
                                    <a href="{% if d_url.url %}{{d_url.url}}{% else %}#{% endif %}" class="" target="{% if d_url.target %}{{d_url.target}}{% else %}_self{% endif %}">
                                        <button class="btn btn-secondary btn-first">
                                                {{ club.deuxieme_texte_savoir_plus }}
                                        </button>
                                    </a>
                                </p>
                            {% endif %}
                        </div>
                        <div class="col-md-6">
                            {% if club.deuxieme_image %}
                                <img class="img_club" src="{{club.deuxieme_image.url}}">
                            {% endif %}
                        </div>
                    </div>
                {% endif %}
                {% if club and (club.troisieme_titre or club.troisieme_contenu) %}
                    <div class="row mt-5">
                        <div class="col-md-6">
                            {% if club.troisieme_image %}
                                <img class="img_club" src="{{club.troisieme_image.url}}">
                            {% endif %}
                        </div>
                        <div class="col-md-6 pl_club class_club_energy">
                            {% if club.troisieme_titre %}
                                <h2 class="text-uppercase  services-h2">
                                    {{ club.troisieme_titre }}
                                </h2>
                            {% endif %}
                            {% if club.troisieme_contenu %}
                                {{ club.troisieme_contenu }}
                            {% endif %}
                            <p class="text-center _pd_tp_40">
                                {% set t_url = club.troisieme_url_savoir_plus %}
                                {% if club.troisieme_texte_savoir_plus %}
                                    <a href="{% if t_url.url %}{{t_url.url}}{% else %}#{% endif %}" class=""  target="{% if t_url.target %}{{t_url.target}}{% else %}_self{% endif %}">
                                        <button class="btn btn-secondary btn-first ">
                                                {{ club.troisieme_texte_savoir_plus }}
                                        </button>
                                    </a>
                                {% endif %}
                            </p>
                        </div>
                    </div>
                {% endif %}
            {% else %}
                {% if club and (club.premier_titre or club.premier_contenu) %}
                    <div class="row">
                        <div class="col-md-6 class_club_energy">
                            {% if club.premier_titre %}
                                <h2 class="text-uppercase  services-h2">
                                    {{club.premier_titre}}
                                </h2>
                            {% endif %}
                            {{club.premier_contenu}}
                            {% set p_url = club.premier_url_savoir_plus %}
                            {% if club.premier_texte_savoir_plus %}
                                <p class="text-center _pd_tp_40">
                                    <a href="{% if p_url.url %}{{p_url.url}}{% else %}#{% endif %}" class="" target="{% if p_url.target %}{{p_url.target}}{% else %}_self{% endif %}">
                                        <button class="btn btn-secondary btn-first">
                                                {{ club.premier_texte_savoir_plus }}
                                        </button>
                                    </a>
                                </p>
                            {% endif %}
                        </div>
                        <div class="col-md-6 pl_club">
                            {% if club.premier_image %}
                                <img class="img_club" src="{{club.premier_image.url}}">
                            {% endif %}
                        </div>
                    </div>
                {% endif %}
                {% if club and (club.deuxieme_titre or club.deuxieme_contenu) %}
                    <div class="row mt-5">
                        <div class="col-md-6">
                            {% if club.deuxieme_image %}
                                <img class="img_club" src="{{club.deuxieme_image.url}}">
                            {% endif %}
                        </div>
                        <div class="col-md-6 pl_club class_club_energy">
                            {% if club.deuxieme_titre %}
                                <h2 class="text-uppercase  services-h2">
                                    {{club.deuxieme_titre}}
                                </h2>
                            {% endif %}
                            {{ club.deuxieme_contenu }}
                            <p class="text-center _pd_tp_40">
                                {% set d_url = club.deuxieme_url_savoir_plus %}
                                {% if club.deuxieme_texte_savoir_plus %}
                                    <a href="{% if d_url.url %}{{d_url.url}}{% else %}#{% endif %}" class="" target="{% if d_url.target %}{{d_url.target}}{% else %}_self{% endif %}">
                                            <button class="btn btn-secondary btn-first">
                                                {{ club.deuxieme_texte_savoir_plus }}
                                            </button>
                                    </a>
                                {% endif %}
                            </p>
                        </div>
                    </div>
                {% endif %}
                {% if club and (club.troisieme_titre or club.troisieme_contenu) %}
                    <div class="row mt-5">
                        <div class="col-md-6 class_club_energy">
                            {% if club.troisieme_titre %}
                                <h2 class="text-uppercase  services-h2">
                                    {{ club.troisieme_titre }}
                                </h2>
                            {% endif %}
                            {{ club.troisieme_contenu }}
                            <p class="text-center _pd_tp_40">
                                {% set t_url = club.troisieme_url_savoir_plus %}
                                {% if club.troisieme_texte_savoir_plus %}
                                    <a href="{% if t_url.url %}{{t_url.url}}{% else %}#{% endif %}" class=""  target="{% if t_url.target %}{{t_url.target}}{% else %}_self{% endif %}">
                                        <button class="btn btn-secondary btn-first ">
                                                {{ club.troisieme_texte_savoir_plus }}
                                        </button>
                                    </a>
                                {% endif %}
                            </p>
                        </div>
                        <div class="col-md-6 pl_club">
                            {% if club.troisieme_image %}
                                <img class="img_club" src="{{club.troisieme_image.url}}">
                            {% endif %}
                        </div>
                    </div>
                {% endif %}
            {% endif %}
        {% endfor %}
    </div>
</div>
{% endblock %} 